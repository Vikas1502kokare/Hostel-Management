{
  "_version": "1.76.0",
  "sap.app": {
    "id": "hostel.com.hostelmanagement",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.1",
      "toolsId": "b9ae0d1d-a302-4c88-902a-e444bec0d24e"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v2/catalog/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.141.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
         "sap.ui.unified": {},
           "sap.ui.comp": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "hostel.com.hostelmanagement.i18n.i18n",
          "supportedLocales": ["en", "de", ""],
            "fallbackLocale": "en"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "useBatch": false
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "hostel.com.hostelmanagement.view",
        "async": true,
        "viewPath": "hostel.com.hostelmanagement.view"
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": ":?query:",
          "target": [
            "TargetView1"
          ]
        },
        {
          "name": "BookingPage",
          "pattern": "BookingPage",
          "target": [
            "BookingPage"
          ]
        },
                {
          "name": "RoomsDetails",
          "pattern": "RoomsDetails",
          "target": [
            "RoomsDetails"
          ]
        },

                        {
          "name": "PriceList",
          "pattern": "PriceList",
          "target": [
            "PriceList"
          ]
        },
        {
          "name": "AdminTile",
          "pattern": "AdminTile",
          "target": [
            "AdminTile"
          ]
        },
        {
          "name": "Login",
          "pattern": "Login",
          "target": [
            "Login"
          ]
        }
      ],
      "targets": {
        "TargetView1": {
          "id": "View1",
          "name": "View1"
        },
        "BookingPage": {
          "id": "BookingPage",
          "name": "BookingPage"
        },
        "PriceList": {
          "id": "PriceList",
          "name": "PriceList"
        },      
        "RoomsDetails": {
          "id": "RoomsDetails",
          "name": "RoomsDetails"
        },
        
        "AdminTile": {
          "id": "AdminTile",
          "name": "AdminTile"
        },
        "Login": {
          "id": "Login",
          "name": "Login"
        }
      }
    },
    "rootView": {
      "viewName": "hostel.com.hostelmanagement.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}